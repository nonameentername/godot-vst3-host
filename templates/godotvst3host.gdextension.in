[godot-vst3-host]

version = "${version}"
build = "${build}"

[configuration]

entry_symbol = "godot_vst3_host_library_init"
compatibility_minimum = 4.5
reloadable = true

[libraries]

linux.debug.x86_64 = "res://addons/vst3-host/bin/linux/libvst3hostgodot.linux.template_debug.dev.x86_64.so"
linux.release.x86_64 = "res://addons/vst3-host/bin/linux/libvst3hostgodot.linux.template_release.x86_64.so"
windows.debug.x86_64 = "res://addons/vst3-host/bin/windows/libvst3hostgodot.windows.template_debug.dev.x86_64.dll"
windows.release.x86_64 = "res://addons/vst3-host/bin/windows/libvst3hostgodot.windows.template_release.x86_64.dll"
macos.debug = "res://addons/vst3-host/bin/macos/macos.framework/libvst3hostgodot.macos.template_debug"
macos.release = "res://addons/vst3-host/bin/macos/macos.framework/libvst3hostgodot.macos.template_release"
